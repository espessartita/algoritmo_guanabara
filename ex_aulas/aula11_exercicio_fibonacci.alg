algoritmo "aula11_combinacoes"
var
   n1, n2, n3, c: inteiro
inicio
      n1 <- 0
      escreva(n1)
      n2 <- 1
      escreva (n2)
      para c <- 3 ate 15 faca
           n3 <- n1 + n2
           escreva (n3)
           n1 <- n2
           n2 <- n3
      fimpara
           
fimalgoritmo
